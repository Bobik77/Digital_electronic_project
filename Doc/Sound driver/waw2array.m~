[y,Fs] = audioread("bump.wav");
maximal = max(y);
normalised = y ./ maximal;
y_int = round(normalised.*127 + 127);
output_data = y_int(:,1)

fid = fopen( 'sound_string.txt', 'wt' );
for n = 0:size(output_data)
  [a1,a2,a3,a4] = ProcessMyImage( image );
  fprintf( fid, ' ,', a1, a2, a3, a4);
end
fclose(fid);